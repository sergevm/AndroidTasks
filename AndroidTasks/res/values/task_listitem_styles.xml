<?xml version="1.0" encoding="utf-8"?>
<resources>
<style name="ListTaskIcon">
	<item name="android:layout_width">30sp</item>
	<item name="android:layout_height">wrap_content</item>
	<item name="android:paddingRight">5dp</item>
</style>
<style name="ListReminderIcon">
	<item name="android:layout_width">25sp</item>
	<item name="android:layout_height">25sp</item>
	<item name="android:paddingRight">5sp</item>
</style>
<style name="ListDescription" parent="style/TextAppearance">
	<item name="android:textSize">16sp</item>
	<item name="android:layout_width">fill_parent</item>
	<item name="android:layout_height">wrap_content</item>
	<item name="android:maxLength">80</item>
	<item name="android:ellipsize">end</item>
	<item name="android:maxLines">1</item>
</style>
<style name="ListReminderDate" parent="style/TextAppearance">
	<item name="android:textSize">10sp</item>
	<item name="android:textColor">#FFCC33</item>
	<item name="android:layout_height">wrap_content</item>
	<item name="android:layout_width">wrap_content</item>
</style>
<style name="ListTargetDate" parent="style/TextAppearance">
	<item name="android:layout_weight">1</item>
	<item name="android:textSize">12sp</item>
	<item name="android:layout_height">wrap_content</item>
	<item name="android:layout_width">wrap_content</item>
</style>
</resources>
